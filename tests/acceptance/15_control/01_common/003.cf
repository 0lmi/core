bundle agent init
{
vars:
	"dummy" string => "dummy";
}

#######################################################

bundle agent test
{
vars:
	"dummy" string => "dummy";
}

#######################################################

bundle agent check
{
vars:
	"dummy" string => "dummy";

classes:
	"ok" expression => "any";

reports:
    ok::
	"$(this.promise_filename) Pass";
    !ok::
	"$(this.promise_filename) FAIL";
}

