#######################################################
#
# Test ignore_missing_bundles, expect to pass
#
#######################################################

body common control
{
  ignore_missing_bundles => "on";

  inputs => { "../../default.cf" };
  bundlesequence  => { "g", "missing", default("$(this.promise_filename)") };   
  version => "1.0";
nova_edition::
  host_licenses_paid => "5";
}

bundle common g
{
vars:
	"dummy" string => "dummy";
}

